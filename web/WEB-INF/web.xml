<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>StartServlet</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>StartServlet</servlet-name>
        <servlet-class>control.StartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartServlet</servlet-name>
        <url-pattern>/StartServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NavigateServlet</servlet-name>
        <servlet-class>control.NavigateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NavigateServlet</servlet-name>
        <url-pattern>/NavigateServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProductServlet</servlet-name>
        <servlet-class>control.ProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductServlet</servlet-name>
        <url-pattern>/ProductServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CartServlet</servlet-name>
        <servlet-class>control.CartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CartServlet</servlet-name>
        <url-pattern>/CartServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>control.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AdminStartServlet</servlet-name>
        <servlet-class>control.AdminStartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminStartServlet</servlet-name>
        <url-pattern>/AdminStartServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ForgotPasswordServlet</servlet-name>
        <servlet-class>control.ForgotPasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ForgotPasswordServlet</servlet-name>
        <url-pattern>/ForgotPasswordServlet</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>VNPayAjax</servlet-name>
        <servlet-class>control.VNPayAjax</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VNPayAjax</servlet-name>
        <url-pattern>/VNPayAjax</url-pattern>
    </servlet-mapping>
    
    
</web-app>
